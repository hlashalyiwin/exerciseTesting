<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= title %> </h1>
    <table>
      <thead>
        <th>Name:</th>
        <th>Email:</th>
        <th>Password:</th>
      </thead>
      <tbody>
        <% for(var i=0; i< user.length; i++){ %>
        <tr>
            <td><a href="/users/userdetail/<%= user[i]._id %>"> <%= user[i].name %></a></td>
            <td><%= user[i].email %></td>
            <td><%= user[i].password %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
